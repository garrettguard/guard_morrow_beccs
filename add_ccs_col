add_ccs_col <- function(tibble) {tibble <- tibble %>%
     mutate( SSP = case_when(
         grepl("NO_CCS", scenario, fixed=TRUE) ~ "NO_CCS",
         grepl("NO_FF_CCS", scenario, fixed=TRUE) ~ "NO_FF_CCS",
         grepl("NO_BECCS", scenario, fixed=TRUE) ~ "NO_BECCS",
         TRUE                                 ~ "ALL_CCS"
     )) }
